{
  "name": "Lazyhub",
  "version": "0.0.0",
  "manifest_version": 3,
  "permissions": ["scripting", "tabs"],
  "host_permissions": [
    "https://github.com/*"
  ],
  "action": {
    "default_popup": "menu.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}